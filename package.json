{
    "private": true,
    "name": "fit3161",
    "description": "An application to assist in the assessment of post-traumatic amnesia",
    "scripts": {
        "build": "next build",
        "develop": "next dev",
        "format": "prettier --write .",
        "start": "next start"
    },
    "dependencies": {
        "emotion": "npm:@emotion/react@^11.1.5",
        "next": "^10.1.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "typescript": "^4.2.3"
    },
    "devDependencies": {
        "@types/node": "^14.14.37",
        "@types/react": "^17.0.3",
        "babel-plugin-emotion": "npm:@emotion/babel-plugin@^11.3.0",
        "prettier": "^2.2.1"
    },
    "babel": {
        "presets": [
            [
                "next/babel",
                {
                    "preset-react": {
                        "runtime": "automatic",
                        "importSource": "emotion"
                    }
                }
            ]
        ],
        "plugins": [
            "emotion"
        ]
    },
    "prettier": {
        "printWidth": 100,
        "tabWidth": 4
    }
}
